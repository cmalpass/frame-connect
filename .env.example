# Environment Configuration for Frameo Sync

# Server
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# Database
DATABASE_PATH=./data/frameo.db

# Photo storage (temp files during sync)
PHOTOS_PATH=./photos

# ADB Configuration
ADB_HOST=localhost
ADB_PORT=5037

# Google OAuth (required for Google Photos integration)
# Get credentials from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:3000/api/oauth/google/callback
